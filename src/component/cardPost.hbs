{{#with data}}
	<div class="list">
		<div class="post">
			<strong class="post_title">{{title}}</strong>
			<div class="post_info">{{date}} by <span class="post_autor">{{author}}</span></div>
			<p class="post_description">{{description}}</p>
			<a href="{{url}}">Continue reading</a>
		</div> 
	</div>
{{/with}}